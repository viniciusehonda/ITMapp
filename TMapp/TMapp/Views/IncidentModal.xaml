<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TMapp.Views.IncidentModal">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="xLabel"  Text="Categoria:"/>
            <Label x:Name="xLblTipo"  Text="{Binding VlblTipo}"/>
            <Label x:Name="xLabel2"  Text="Descrição:"/>
            <Label x:Name="xLblDescricao"  Text="{Binding VlblDescricao}"/>
            <Label x:Name="xLabel3"  Text="Data:"/>
            <Label x:Name="xLblData"/>
            <Label x:Name="xLabel4" Text="Hora:"/>
            <Label x:Name="xLblHora"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>